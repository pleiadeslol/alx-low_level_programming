#include "main.h"
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

/**
 * int _isupper - function that checks for uppercase character
 *
 * @c: character checked
 *
 * Return: 1 if c is uppercase, 0 otherwise
 */

int _isupper(int c)
{
	int isupper(int c);

	if (isupper(c) == 0)
	{
		return (0);
	}
	else
	{
		return (1);
	}
}
